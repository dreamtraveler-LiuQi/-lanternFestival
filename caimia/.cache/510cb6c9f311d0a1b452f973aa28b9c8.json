).text(gameData[gameIndex].gameask)\r\n            $('.stage3 .gametip').text('（'+gameData[gameIndex].gametip+'）')\r\n        }else{\r\n            let result = {}\r\n            result.usetime = gameData.\r\n                map(data=>30-data.time).\r\n                reduce((prev,curr)=>prev+curr)\r\n            result.rightcount = gameData.\r\n                map(data=>data.right).\r\n                filter(data=>data).\r\n                length\r\n\r\n            if(window.callback)window.callback(result)\r\n        }\r\n    }\r\n    var showTime = (function(){\r\n        var elem = $('.showTime')\r\n        var timmer = null\r\n        var chinaNum = ['一','二','三','四','五','六','七','八','九','十']\r\n        var indexnumber = $('.indexnumber')\r\n        return function(){\r\n            clearInterval(timmer)\r\n            elem.text(30)\r\n            if(gameData[gameIndex]){\r\n                indexnumber.text(chinaNum[gameIndex])\r\n                timmer = setInterval(function(){\r\n                    if(gameData[gameIndex]&&gameData[gameIndex].time>0){\r\n                        elem.text(--gameData[gameIndex].time)\r\n                    }\r\n                },1000)\r\n            }\r\n        }\r\n    })()\r\n    showTime()\r\n    $('.stage3 .gameask').text(gameData[gameIndex].gameask)\r\n    $('.stage3 .gametip').text('（'+gameData[gameIndex].gametip+'）')\r\n}\r\n\r\n\r\n\r\n\r\nconst gameData = [\r\n    {\r\n        gameask:'皇帝从不上早朝',\r\n        gametip:'打一三国名仕的号',\r\n        gameans:'卧龙',\r\n        time:30,\r\n        right:false\r\n    },\r\n    {\r\n        gameask:'皇帝从不上早朝2',\r\n        gametip:'打一三国名仕的号2',\r\n        gameans:'卧龙2',\r\n        time:30,\r\n        right:false\r\n    },\r\n    {\r\n        gameask:'皇帝从不上早朝3',\r\n        gametip:'打一三国名仕的号3',\r\n        gameans:'卧龙3',\r\n        time:30,\r\n        right:false\r\n    },\r\n    {\r\n        gameask:'皇帝从不上早朝4',\r\n        gametip:'打一三国名仕的号4',\r\n        gameans:'卧龙4',\r\n        time:30,\r\n  